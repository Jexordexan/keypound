{
  "name": "keypound",
  "version": "0.1.0",
  "description": "A library for handling keyboard shortcuts through multiple levels of an app.",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack ./src/index.js keypound.js",
    "dev": "webpack-dev-server --progress --watch --colors",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "fix": "eslint src/**/*.js --fix",
    "lint": "eslint src/**/*.js",
    "test": "npm run lint; npm run unit"
  },
  "keywords": [
    "key",
    "press",
    "shourtcut",
    "hotkey",
    "context"
  ],
  "author": "Jordan Simonds <simonds.jordan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.1",
    "eslint": "^4.4.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.4",
    "loglevel": "^1.4.1",
    "phantomjs-prebuilt": "^2.1.14",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "jasmine-core": "^2.7.0",
    "karma-jasmine": "^1.1.0"
  }
}
